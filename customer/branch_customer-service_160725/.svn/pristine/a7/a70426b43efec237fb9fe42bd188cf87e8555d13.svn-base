dubbo.container=logback,spring

#
# 服务名。
#
dubbo.application.name=user-services

#
#
#
dubbo.spring.config=classpath*:/META-INF/spring/spring-context.xml

#
# 服务注册中心地址。
#
dubbo.registry.address=zookeeper://10.0.2.21:2181

#dubbo.spring.config=classpath*:spring-context.xml

#
# 服务注册IP。
#
dubbo.protocol.host=127.0.0.1

#
# 服务监听端口。
#
dubbo.protocol.port=20880

#
# 日志文件目录.
#
dubbo.logback.file=/data/dubbo/user-service/logs/dubbo-provider.log

#
#
#
dubbo.registry.file=/data/dubbo/user-service/conf/dubbo-registry-cache.properties

#
# 日志级别.
#
dubbo.log4j.level=INFO
