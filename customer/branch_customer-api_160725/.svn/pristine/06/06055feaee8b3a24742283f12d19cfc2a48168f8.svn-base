    /**
     * 产品价 - 普通票价
     * 
     * @param from
     * @return
     */
    public static CusProductPriceVO proReleaseToProPrice(ProductRelease from) {
        CusProductPriceVO to = new CusProductPriceVO();
        if (from == null) {
            return to;
        }
        to.setAuditStatus(from.getAuditStatus());
        to.setCheckInType(from.getCheckInType());
        to.setCreateBy(from.getCreateBy());
        to.setCreateDate(from.getCreateDate());
        to.setEndTime(from.getEndTime());
        to.setFlag(from.getFlag());
        to.setHourNode(from.getHourNode());
        to.setId(from.getId());
        to.setName(from.getName());
        to.setPackageStatus(from.getPackageStatus());
        to.setProCategory(from.getProCategory());
        to.setProductChannel(from.getProductChannel());
        to.setProductClassify(from.getProductClassify());
        to.setProductCode(from.getProductCode());
        to.setProductId(from.getProductId());
        to.setProductKind(from.getProductKind());
        to.setProductPrice(from.getProductPrice());
        to.setProductType(from.getProductType());
        to.setReason(from.getReason());
        to.setRegion(from.getRegion());
        to.setRemarks(from.getRemarks());
        to.setRonda(from.getRonda());
        to.setSort(from.getSort());
        to.setStartTime(from.getStartTime());
        to.setSupplierId(from.getSupplierId());
        to.setUpdateBy(from.getUpdateBy());
        to.setUpdateDate(from.getUpdateDate());
        to.setMinutesNode(from.getMinutesNode());
        return to;
    }
