				</if>
				<if test="relation.rebateId != null">
					relation.rebate_id = #{relation.rebateId,jdbcType=BIGINT}
				</if>
				<if test="releasee != null">
					<if test="releasee.name != null">
						releasee.name like #{releasee.name,jdbcType=VARCHAR}
					</if>
				</if>
				<if test="product != null">
